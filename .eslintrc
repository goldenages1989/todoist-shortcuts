{
    "parserOptions": {
        "ecmaVersion": 2017,
    },
    "extends": "google",
    "rules": {
      "require-jsdoc": "off",
      "spaced-comment": "off",
      "guard-for-in": "off",
      "brace-style": ["error", "1tbs", { "allowSingleLine": true }],
      "block-spacing": ["error", "always"],
    }
}
